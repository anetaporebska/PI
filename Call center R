
#include <stdio.h>
#include <stdlib.h>


int main()
{
    int n;
    char type;
    scanf("%d",&n);

    char **numbers;
    numbers=malloc(sizeof(numbers)*n);
    char *block;
    block=malloc(sizeof(block)*17);
    for(int i=0; i<n; i+=1){
        numbers[i]=block+i*(17);        //Aneta, prosze powiedz, jak to dziala. Ja ne panemaju :c
    }

    int *t;
    t=malloc(sizeof(t)*(n+1));

    int *tr;
    tr=malloc(sizeof(tr)*n);

    int i=0;
    int j=0;
    int top=0;
    int la=0;
    int lr=0;
    while(n>0){
        char* type='a';     //czemu wskaznik? I czemu przypisanie od razu? Zwlaszcza ze inicjalizacja zmiennej byla w 9 linii
        scanf("%s",&type);
                            //prawdopodobnie przydatne czyszczenie bufora
        if(type=='a'){
            scanf("%d",&t[i]);
            scanf("%s",numbers[i]);     //tutaj tez radze wyczyscic bufor
            i+=1;
            la+=1;
        }
        else if(type=='r'){

            int tim;
            scanf("%d", &tim);
            if(la>lr) {
                int ocz = tim - t[top];
                tr[j] = ocz;
                j += 1;
                top += 1;
                lr += 1;
            }
        }

        n-=1;
    }
    top=0;
    for(int i=0; i<la && i<lr; i+=1){
        printf("%s ",numbers[top]);
        printf("%d \n",tr[i]);      //Nie wiem, czy sprawdzarka nie moze czepic sie tego bialego znaku miedzy wartoscia a nowa linia xD
        top+=1;
    }


    return 0;
}



