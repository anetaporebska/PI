

#include <stdio.h>
#include <stdlib.h>
#include <string.h>



int main()
{
   
    char tekst[100];
    
    fgets(tekst, sizeof tekst, stdin);
    
    int s=0;
    int i=0;
    while(tekst[i]!=' '){
        s+=1;
        i+=1;
    }
    
    for(int i=0; i<sizeof(tekst); i+=1){
        if(tekst[i]==' '){
            printf("%c",tekst[i]);
        }    
        else{
            if(tekst[i]<'Z'){
                tekst
            }
            
            tekst[i]+=abs();
            printf("%c",tekst[i]);
        }
    }    
    
    

    return 0;
}
